<div class="control-group">
    <label class="control-label">Title </label>
    <div class="controls">
       <%=f.text_field :eng_title ,:class=>"span5 m-wrap"%>
       <!-- <div class="pull-right"> Required Fields </div> -->
    </div>
 </div>
 <div class="control-group">
    <label class="control-label">English Title </label>
    <div class="controls">
     <%= f.cktext_area :eng_content, :class => 'someclass', :ckeditor => {:language => 'eng'} %>

       <!-- <input type="text" class="span5 m-wrap" /> -->
       <!-- <div class="pull-right"> Required Fields </div> -->
    </div>
 </div>
<div class="control-group">
  <label class="control-label">Status </label>
    <div class="controls">
       <%= f.select :state,StaticPage::STATES.map{|k,v| [v,k]} ,:selected =>f.object.new_record? ? 1 : @static_page.state ? 1 : 0 %>
    </div>
</div>

<div class="form-actions">
  <%=f.submit "Save",:class=> "btn blue"%>
  <button type="button" class="btn" onclick="this.form.reset();">Reset</button>
  <button type="button" class="btn" onclick="window.location.href='/admin/static_pages'">Cancel</button>
</div>